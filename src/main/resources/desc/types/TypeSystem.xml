			<name>eu.skqs.type.Tei</name>
			<description>Root type of TEI documents</description>
			<supertypeName>uima.tcas.Annotation</supertypeName>
			<features>
				<featureDescription>
					<name>title</name>
					<description>Title of this text (chinese).</description>
					<rangeTypeName>uima.cas.String</rangeTypeName>
				</featureDescription>
				<featureDescription>
					<name>titleEn</name>
